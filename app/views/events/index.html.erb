
<h1>ALL EVENTS</h1>
	<div class="container">
		<% @events.each do |event| %>
			<%= event.title %>
			<%= event.starts_at %>
			<%= link_to 'Show event', event_path(event)%>
		<% end %>
	</div>
<%= link_to 'Add event', new_event_path %>

<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>